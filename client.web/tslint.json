{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "no-console": false,
    "jsx-no-lambda": [ false ],
    "no-var-requires": false,
    "no-require-imports": false,
    "ordered-imports": false
  }
}

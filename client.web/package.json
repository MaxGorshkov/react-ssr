{
  "name": "react-ssr-client",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "axios": "0.18.0",
    "node-sass-chokidar": "1.3.4",
    "npm-run-all": "4.1.3",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-redux": "5.0.7",
    "react-router-config": "4.4.0-beta.1",
    "react-router-redux": "5.0.0-alpha.9",
    "react-scripts-ts": "3.1.0",
    "recompose": "0.30.0",
    "redux": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-dev": "set PORT=3000 && react-scripts-ts start",
    "build-js": "react-scripts-ts build",
    "build": "npm-run-all build-css build-js",
    "start": "npm-run-all -p watch-css start-dev",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "23.3.3",
    "@types/node": "10.11.4",
    "@types/react": "16.4.15",
    "@types/react-dom": "16.0.8",
    "@types/react-redux": "6.0.9",
    "@types/react-router-config": "1.0.9",
    "@types/react-router-redux": "5.0.16",
    "@types/recompose": "0.26.5",
    "@types/redux-logger": "3.0.6",
    "typescript": "3.1.1"
  }
}
